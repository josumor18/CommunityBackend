<p id="notice"><%= notice %></p>

<h1>Device Tokens</h1>

<table>
  <thead>
    <tr>
      <th>Id user</th>
      <th>Token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @device_tokens.each do |device_token| %>
      <tr>
        <td><%= device_token.id_user %></td>
        <td><%= device_token.token %></td>
        <td><%= link_to 'Show', device_token %></td>
        <td><%= link_to 'Edit', edit_device_token_path(device_token) %></td>
        <td><%= link_to 'Destroy', device_token, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device Token', new_device_token_path %>
